{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Travel</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Title</th>
                <td>{{ entity.Title }}</td>
            </tr>
            <tr>
                <th>Fromdate</th>
                <td>{{ entity.fromDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Todate</th>
                <td>{{ entity.toDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('travel') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('travel_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
