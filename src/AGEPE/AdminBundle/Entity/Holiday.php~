<?php

namespace AGEPE\AdminBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Holiday
 */
class Holiday
{
    /**
     * @var \DateTime
     */
    private $holidayDate;

    /**
     * @var string
     */
    private $title;

    /**
     * @var integer
     */
    private $id;


    /**
     * Set holidayDate
     *
     * @param \DateTime $holidayDate
     * @return Holiday
     */
    public function setHolidayDate($holidayDate)
    {
        $this->holidayDate = $holidayDate;
    
        return $this;
    }

    /**
     * Get holidayDate
     *
     * @return \DateTime 
     */
    public function getHolidayDate()
    {
        return $this->holidayDate;
    }

    /**
     * Set title
     *
     * @param string $title
     * @return Holiday
     */
    public function setTitle($title)
    {
        $this->title = $title;
    
        return $this;
    }

    /**
     * Get title
     *
     * @return string 
     */
    public function getTitle()
    {
        return $this->title;
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }
}